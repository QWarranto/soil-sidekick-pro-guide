{
  "name": "@soilsidekick/sdk-tools",
  "version": "1.0.0",
  "description": "SDK generation and testing tools for SoilSidekick Pro API",
  "scripts": {
    "generate:all": "npm run generate:typescript && npm run generate:python && npm run generate:go",
    "generate:typescript": "openapi-generator-cli generate -i ../openapi-spec.yaml -g typescript-fetch -o ./generated/typescript --additional-properties=npmName=@soilsidekick/sdk,npmVersion=1.1.0,supportsES6=true,typescriptThreePlus=true",
    "generate:python": "openapi-generator-cli generate -i ../openapi-spec.yaml -g python -o ./generated/python --additional-properties=packageName=soilsidekick,packageVersion=1.1.0,projectName=soilsidekick-sdk",
    "generate:go": "openapi-generator-cli generate -i ../openapi-spec.yaml -g go -o ./generated/go --additional-properties=packageName=soilsidekick,packageVersion=1.1.0",
    "generate:ruby": "openapi-generator-cli generate -i ../openapi-spec.yaml -g ruby -o ./generated/ruby --additional-properties=gemName=soilsidekick,gemVersion=1.1.0",
    "generate:java": "openapi-generator-cli generate -i ../openapi-spec.yaml -g java -o ./generated/java --additional-properties=artifactId=soilsidekick-sdk,groupId=com.soilsidekick,artifactVersion=1.1.0",
    "generate:php": "openapi-generator-cli generate -i ../openapi-spec.yaml -g php -o ./generated/php --additional-properties=packageName=SoilSidekick,invokerPackage=SoilSidekick",
    "test": "npx ts-node test-sdk.ts",
    "validate": "openapi-generator-cli validate -i ../openapi-spec.yaml",
    "clean": "rm -rf ./generated/*"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.7.0",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}
